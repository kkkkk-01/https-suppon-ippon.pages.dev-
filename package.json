{
  "name": "suppon-ippon",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:sandbox": "wrangler pages dev dist --d1=suppon-ippon-production --ip 0.0.0.0 --port 3000",
